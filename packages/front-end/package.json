{
  "name": "front-end",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:node": "cross-env NODE_ENV=development nodemon ./build/index.js --watch ./build/index.js",
    "dev:remix": "remix watch",
    "lint": "TIMING=1 eslint \"src/**/*.{js,jsx}\"",
    "start": "cross-env NODE_ENV=production node ./build/index.js"
  },
  "dependencies": {
    "@remix-run/express": "^1.3.4",
    "@remix-run/node": "^1.3.4",
    "@remix-run/react": "^1.3.4",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.4",
    "@remix-run/eslint-config": "^1.3.4",
    "eslint": "^8.11.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5"
  },
  "engines": {
    "node": ">=14"
  }
}
